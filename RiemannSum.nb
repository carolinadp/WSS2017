                  RowBox[{
                   RowBox[{"{", 
                    RowBox[{"xStart", ",", " ", "0"}], "}"}], ",", " ", 
                   RowBox[{"{", 
                    RowBox[{"xStart", ",", " ", "yCurrent"}], "}"}], ",", " ", 
