Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "Obtains", " ", "the", " ", "area", " ", "of", " ", "all", " ", 
    "rectangles", " ", "in", " ", "the", " ", "Riemann", " ", "sum"}], "*)"}],
   "\n", 
  RowBox[{
   RowBox[{
    RowBox[{"riemannArea", " ", "[", 
     RowBox[{
     "function_", ",", " ", "start_", ",", " ", "end_", ",", " ", 
      "rectWidth_", ",", " ", 
      RowBox[{"type_String:", "\"\<Left\>\""}]}], "]"}], ":=", " ", 
    RowBox[{"Plus", " ", "@@", "\[NonBreakingSpace]", 
     RowBox[{"Table", " ", "[", "\n", "\t", 
      RowBox[{
       RowBox[{"rectWidth", " ", "*", " ", 
        RowBox[{"Switch", "[", "\[IndentingNewLine]", "\t\t", 
         RowBox[{
         "type", ",", "\[IndentingNewLine]", "\t\t", "\"\<Left\>\"", ",", 
          "\[IndentingNewLine]", "\t\t", 
          RowBox[{"function", "[", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"x", " ", "-", " ", "1"}], ")"}], " ", "*", " ", 
            "rectWidth"}], "]"}], ",", "\[IndentingNewLine]", "\t\t", 
          "\"\<Right\>\"", ",", "\[IndentingNewLine]", "\t\t", 
          RowBox[{"function", " ", "@", " ", 
           RowBox[{"Min", "[", 
            RowBox[{
             RowBox[{"xStart", " ", "+", " ", "rectWidth"}], ",", "end"}], 
            "]"}]}], ",", "\[IndentingNewLine]", "\t\t", "\"\<Mean\>\"", ",", 
          "\[IndentingNewLine]", "\t\t", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"function", "[", 
              RowBox[{
               RowBox[{"(", 
                RowBox[{"x", " ", "-", " ", "1"}], ")"}], " ", "*", " ", 
               "rectWidth"}], "]"}], " ", "+", " ", 
             RowBox[{"function", " ", "@", " ", 
              RowBox[{"Min", "[", 
               RowBox[{
                RowBox[{"xStart", " ", "+", " ", "rectWidth"}], ",", "end"}], 
               "]"}]}]}], ")"}], " ", "/", " ", "2.0"}]}], 
         "\[IndentingNewLine]", "\t", "]"}]}], ",", "\[IndentingNewLine]", 
       "\t", 
       RowBox[{"{", 
        RowBox[{"x", ",", " ", "1", ",", " ", 
         RowBox[{"Ceiling", "[", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"end", "-", "start"}], ")"}], "/", "rectWidth"}], "]"}]}],
         "}"}]}], "\[IndentingNewLine]", "]"}]}]}], ";"}]}]], "Code",
